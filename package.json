{
  "name": "reusable-react",
  "version": "1.1.0",
  "author": {
    "name": "William Phelps",
    "email": "williamphelps13@gmail.com"
  },
  "license": "MIT",
  "description": "A design driven component library built with top-notch tooling: React 18, Tailwind, Storybook, Chromatic and Cypress Testing, and corresponding Figma assets",
  "keywords": [
    "Reusable Components",
    "Component Library",
    "React",
    "Tailwind",
    "Storybook",
    "Chromatic",
    "Cypress",
    "Design System",
    "Figma"
  ],
  "homepage": "https://reusable-react.williamjphelps.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/williamphelps13/reusable-react"
  },
  "main": "./dist/reusable-react.umd.js",
  "module": "./dist/reusable-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/reusable-react.es.js",
      "require": "./dist/vite-component-library-template.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "bugs": {
    "url": "https://github.com/williamphelps13/reusable-react/issues"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slider": "^2.0.6",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "7.6.6",
    "@storybook/test": "^7.6.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-slider": "^1.3.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-loader": "9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-typescript-sort-keys": "^3.1.0",
    "husky": "8.0.3",
    "jsdom": "23.0.1",
    "json": "11.0.0",
    "postcss": "^8.4.32",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "storybook": "^7.6.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "5.0.10",
    "vite-plugin-dts": "3.7.0"
  },
  "files": [
    "dist"
  ]
}
